{
  "name": "n8n-nodes-canvas-lms",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Canvas LMS providing 11 resources and 80+ operations for course management, student enrollment, assignments, grades, and learning analytics.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-canvas-lms.git"
  },
  "homepage": "https://github.com/Velocity-BPA/n8n-nodes-canvas-lms#readme",
  "bugs": {
    "url": "https://github.com/Velocity-BPA/n8n-nodes-canvas-lms/issues"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "canvas",
    "canvas-lms",
    "instructure",
    "learning-management-system",
    "education",
    "lms",
    "e-learning",
    "automation",
    "courses",
    "assignments",
    "grades",
    "enrollments"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/CanvasApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/CanvasLms/CanvasLms.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && cp -r nodes/CanvasLms/*.svg dist/nodes/CanvasLms/",
    "dev": "tsc --watch",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
